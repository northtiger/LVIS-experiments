<?xml version="1.0"?>

<robot name="BoxAtlas">
  <link name="root">
  </link>

  <link name="floor">
    <visual>
      <origin xyz="0.5 0 -0.05"/>
      <geometry>
        <box size="3 1 0.1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.5 0 -0.05"/>
      <geometry>
        <box size="3 1 0.1"/>
      </geometry>
    </collision>
    <collision>
      <geometry>
        <plane normal="0 0 1"/>
      </geometry>
    </collision>
  </link>

  <joint name="root_to_floor" type="fixed">
    <parent link="root"/>
    <child link="floor"/>
  </joint>

  <link name="wall">
    <visual>
      <origin xyz="-1.05 0 1"/>
      <geometry>
        <box size="0.1 1 2.2"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="-1.05 0 1"/>
      <geometry>
        <box size="0.1 1 2"/>
      </geometry>
    </collision>
    <collision>
      <origin xyz="-1 0 1"/>
      <geometry>
        <plane normal="1 0 0"/>
      </geometry>
    </collision>
  </link>

  <joint name="root_to_wall" type="fixed">
    <parent link="root"/>
    <child link="wall"/>
  </joint>

  <link name="core">
    <inertial>
      <mass value="1.0"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.025" iyy="0.039" izz="0.014" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.025"/>
      <geometry>
        <box size="0.4 0.05 0.55"/>
      </geometry>
    </visual>
  </link>

  <joint name="floating_base" type="planar">
    <axis xyz="0 -1 0"/>
    <parent link="root"/>
    <child link="core"/>
  </joint>

  <link name="rh_dummy">
    <inertial>
      <mass value="0.0"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0" iyy="0.0" izz="0.0" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
    <visual>
      <geometry>
        <sphere radius="0.025"/>
      </geometry>
    </visual>
  </link>

  <link name="rh">
    <inertial>
      <mass value="0.05"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.025" iyy="0.039" izz="0.014" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
    <visual>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0"/>
      </geometry>
    </collision>
  </link>

  <joint name="core_to_rh_rotation" type="revolute">
    <parent link="core"/>
    <child link="rh_dummy"/>
    <origin xyz="0.25 0 0.30"/>
    <axis xyz="0 -1 0"/>
    <limit lower="-1.5708" upper="0.1" velocity="100" effort="2"/>
  </joint>

  <joint name="core_to_rh_extension" type="prismatic">
    <parent link="rh_dummy"/>
    <child link="rh"/>
    <origin xyz="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="0.18" upper="0.75" velocity="100" effort="2"/>
